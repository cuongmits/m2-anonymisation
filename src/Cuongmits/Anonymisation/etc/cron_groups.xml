<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/cron_groups.xsd">
    <group id="toom_anonymisation">
        <schedule_generate_every>60</schedule_generate_every>
        <schedule_ahead_for>1440</schedule_ahead_for>
        <schedule_lifetime>60</schedule_lifetime>
        <history_cleanup_every>4320</history_cleanup_every>
        <history_success_lifetime>10080</history_success_lifetime>
        <history_failure_lifetime>10080</history_failure_lifetime>
        <use_separate_process>0</use_separate_process>
    </group>
</config>
